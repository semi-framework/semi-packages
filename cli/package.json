{
  "name": "@semi-framework/cli",
  "version": "0.1.0",
  "main": "dist/index.js",
  "bin": {
    "semi-cli": "dist/index.js"
  },
  "types": "dist/index.d.ts",
  "description": "The cli package of the Semi-Framework.",
  "repository": "https://github.com/semi-framework/semi-packages.git",
  "homepage": "https://github.com/semi-framework",
  "author": "Laurenz1606 <laurenz1606 <laurenz.rausche@gmail.com>>",
  "license": "MIT",
  "private": false,
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:compile": "npm run dev:format && tsc",
    "build:full": "npm run build:clean && npm run build:compile",
    "dev:format": "prettier --write --config ./package.json src",
    "dev:remove-packages": "rimraf node_modules yarn.lock package-lock.json",
    "dev:start": "ts-node src/index.ts"
  },
  "dependencies": {
    "inquirer": "^8.0.0",
    "slugify": "^1.6.5",
    "ts-node": "^10.9.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.15",
    "@types/yargs": "^17.0.17",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4"
  }
}
